{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "simple-ng-dialog": [
        "dist/simple-ng-dialog"
      ],
      "simple-ng-dialog/*": [
        "dist/simple-ng-dialog/*"
      ],
      "simple-ng-notification": [
        "dist/simple-ng-notification"
      ],
      "simple-ng-notification/*": [
        "dist/simple-ng-notification/*"
      ],
      "simple-ng-date-select": [
        "dist/simple-ng-date-select"
      ],
      "simple-ng-date-select/*": [
        "dist/simple-ng-date-select/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}